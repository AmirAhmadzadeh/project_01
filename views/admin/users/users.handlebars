


<h1>
    users :
</h1>


<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>status</th>
            <th>firstName</th>
            <th>lastName</th>
            <th>Email</th>
            <th>Date</th>
            
    </tr>
    </thead>
     {{#each users}}
     <tbody>
         <tr> 

            <th scope="row">{{_id}}</th>
            <td>status</td>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{email}}</td>
            <td>{{generateTime date}} </td>
          
         </tr>
     </tbody>
     {{/each}}

     
</table>
